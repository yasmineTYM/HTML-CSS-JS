<script>
$(document).ready(function(){
$("t1","t2","t3","t4").text("");
var state = false;
$("#t1").focus(function(){
$(this).css("background","Aliceblue");
if(state == false){
$(this).val('');
}
$("#t1").blur(function(){
$(this).css("background","white");
if ($(this).val() == '') {

 $('#s1').text('邮箱不能为空');  
 $('#s1').append('<img src=image/fail.jpg/>');
 
 $(this).focus();
}
else {
                    if (/^\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/.test($(this).val()) == false) {
                         
						$('#s1').text('邮箱格式不正确，请重新输入');   
						$('#s1').append('<img src=image/fail.jpg/>');                    
                        $(this).focus();
                    }
                    else {
                        $('#s1').text('');
                        $('#s1').append('<img src=image/success.jpg/>');
                        state = true;
                    }
                }
})
})

$("#t2").focus(function(){
$(this).css("background","Aliceblue");
})
$("#t2").blur(function(){
$(this).css("background","white");
var psw1=$(this).val();
var reg=  /^([a-zA-Z0-9]+)$/;
if(psw1=='')
{
$("#s2").text("密码不能为空");
$('#s2').append('<img src=image/fail.jpg/>');
}
else if(reg.test(psw1)){
$("#s2").text("");
$('#s2').append('<img src=image/success.jpg/>');
}
else{

$("#s2").text("只能输入数字和字母，不能含有特殊字符");
$('#s2').append('<img src=image/fail.jpg/>');
}
})

$("#t3").focus(function(){
$(this).css("background","Aliceblue");
})
$("#t3").blur(function(){
$(this).css("background","white");
var psw1=$("#t2").val();
var psw2=$("#t3").val();
if(psw2==''){

$("#s3").text("确认密码不能为空");
$('#s3').append('<img src=image/fail.jpg/>');
}
if(psw1!=psw2){

$("#s3").text("请保证两次密码相同");$('#s3').append('<img src=image/fail.jpg/>');
}
if(psw1==psw2 && psw2!=''){
$("#s3").text();
$('#s3').append('<img src=image/success.jpg/>');
}
})

$("#t4").focus(function(){
$(this).css("background","Aliceblue");
})
$("#t4").blur(function(){
$(this).css("background","white");
var phone=$("#t4").val();
if(phone=='' ||isNaN(phone) || phone.length!=11 ){

$("#s4").text("请输入正确格式的电话号码");
$('#s4').append('<img src=image/fail.jpg/>');
}
else{
$("#s4").text("");
$('#s4').append('<img src=image/success.jpg/>');
}
})
})



$('datetimepicker').datetimepicker({

format:yyyy-mm-dd,

        weekStart: 1,

        todayBtn:  1,

autoclose: 1,

todayHighlight: 1,

startView: 2,

forceParse: 0,

        showMeridian: 1

    });

</script>
